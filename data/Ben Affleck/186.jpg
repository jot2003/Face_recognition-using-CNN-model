    <script>
        function throttle(delay, callback) {
            var previousCall = new Date().getTime();
            return function () {
                var time = new Date().getTime();
                if ((time - previousCall) >= delay) {

                    previousCall = time;
                    callback.apply(null, arguments);
                }
            };
        }
        var listener = throttle(300, function () {
            slotsBeautyCrew.forEach(function (item) {
                var $item = $('#' + item.id);
                if ($item.length) {
                    var $parent = $item.parent();
                    var index = slotsBeautyCrew.indexOf(item);
                    // Make sure wrapping ads in a div that sets visibility based on device
                    if ($parent.is(':visible') && ((window.scrollY || window.pageYOffset) >= ($parent.offset().top - $(window).height() - 450))) {
                        googletag.cmd.push(function () {
                            googletag.pubads().refresh([item.slot]);
                            slotsBeautyCrew.splice(index, 1);
                        });

                    }
                }
            });
        });

        $(window).scroll(listener);


        // trigger scroll event after a short wait to make top leaderborad show without user scroll
        setTimeout(function(){
            $(window).scroll();
        }, 300);
    </script>
